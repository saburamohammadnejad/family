<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ir.maktab.model.services.cfg.xml.EmployeeEntity" table="employee" schema="services">
        <property name="employeeId">
            <column name="employee Id" sql-type="int(11)"/>
        </property>
        <property name="nameEmployee">
            <column name="Name-employee" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="userName">
            <column name="userName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="password">
            <column name="password" sql-type="decimal(10)" precision="10"/>
        </property>
    </class>
</hibernate-mapping>